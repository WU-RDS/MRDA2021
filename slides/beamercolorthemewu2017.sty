\mode<presentation>

%%=======%%
%% WU colours %%
%%=======%%

\definecolor{wublack}{RGB}{0,0,0}
\definecolor{wugrey}{RGB}{137,137,137}
\definecolor{wuwhite}{RGB}{255,255,255}
\definecolor{wublue}{RGB}{0,150,211}


%%==========%%
%% Colour settings %%
%%==========%%

\setbeamercolor{author}{fg=wuwhite,bg=wublue}
\setbeamercolor{logo}{fg=wuwhite,bg=wublue}
\setbeamercolor{title}{fg=wuwhite, bg = wublue}

\setbeamercolor{palette primary}{fg=wublack,bg=wuwhite}
\setbeamercolor{palette secondary}{fg=wugrey,bg=wuwhite}
\setbeamercolor{palette tertiary}{fg=wuwhite,bg=wublue}
\setbeamercolor{palette quaternary}{fg=wublue,bg=wuwhite}

\setbeamercolor{sidebar}{bg=wublue}
\setbeamercolor{palette sidebar primary}{fg=wublue}
\setbeamercolor{palette sidebar secondary}{fg=wublue}
\setbeamercolor{palette sidebar tertiary}{fg=wublue}
\setbeamercolor{palette sidebar quaternary}{fg=wublue}

\setbeamercolor{titlelike}{parent=palette quaternary}

\setbeamercolor{part name}{parent=titlelike}
\setbeamercolor{part title}{parent=title}

\setbeamercolor{structure}{fg=wublack}
\setbeamercolor{alerted text}{fg=wublue}
\setbeamercolor{example text}{fg=wublack} 

\setbeamercolor{block title}{fg=wuwhite,bg=wublue}
\setbeamercolor{block title alerted}{use=alerted text,fg=wuwhite,bg=wublue}
\setbeamercolor{block title example}{use=example text,fg=white,bg=wublue}

\setbeamercolor{block body}{fg=black,bg=wublue!10!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!10!bg}

\setbeamercolor{itemize item}{fg=wublue}
\setbeamercolor{itemize subitem}{parent=itemize item}
\setbeamercolor{itemize subsubitem}{parent=itemize subitem}

\setbeamercolor{section in toc}{fg=wublack,bg=wuwhite}
\setbeamercolor{subsection in toc}{parent=section in toc}
\setbeamercolor{subsubsection in toc}{parent=subsection in toc}

\setbeamercolor{enumerate item}{fg=wublue}


\mode
<all>
